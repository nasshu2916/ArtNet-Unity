#AddressBar {
    position: absolute;
    bottom: 0;
    min-height: 0;
    max-height: 100%;
    height: 50%;
    width: 100%;
    background-color: darkgreen;
}

#AddressValue {
    position: absolute;
    font-size: 14px;
    -unity-font-style: bold;
    -unity-text-align: lower-center;
    left: 0;
    right: 0;
    bottom: 5;
}

#AddressTextPanel {
    font-size: 11px;
    width: 100%;
    height: 100%;
    padding: 5;
}
