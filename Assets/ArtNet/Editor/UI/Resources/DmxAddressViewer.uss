.address-bar {
    position: absolute;
    bottom: 0;
    min-height: 0;
    max-height: 100%;
    width: 100%;
    background-color: darkgreen;
}

.address-text-panel {
    font-size: 11px;
    width: 100%;
    height: 100%;
    padding: 5px;
    justify-content: space-between;
}

.address-value {
    font-size: 14px;
    -unity-font-style: bold;
    -unity-text-align: lower-center;
}
